	<system.serviceModel>
		<services>
			<service name="Vb_BuiltIn_WcfServiceApp.Test_masterService" behaviorConfiguration="Vb_BuiltIn_WcfServiceApp.Test_masterServiceBehavior">
				<!-- Service Endpoints -->
				<endpoint address="" binding="wsHttpBinding" contract="Vb_BuiltIn_WcfServiceApp.ITest_masterService">
					<!-- 
					Upon deployment, the following identity element should be removed or replaced to reflect the 
